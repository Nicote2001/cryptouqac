@{
    ViewData["Title"] = "Home Page";
}
@model List<projet2uqac.Models.Key>;
<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
    <button></button>
</div>
 <form asp-action="GenerateKey" method="get">
    <button type="submit">Generer mes clées</button>
   </form>
<div class="sideList ">
    @for (int i = 0; i < Model.Count(); i++)
    {

        <a>@Model[i].name</a> <button onclick="window.location.href = '@Url.Action("Export","Home",new{id=Model[i].id})';">Export</button>

    }
</div>
<button onclick="window.location.href = '@Url.Action("Import","Home")';">IMPORT KEY</button>
